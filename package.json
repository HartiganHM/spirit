{
  "name": "spirit-backend",
  "version": "1.0.0",
  "description": "",
  "main": "knexfile.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "test:client": "cd client && npm test && cd..",
    "test:client:ci": "cd client && CI=true npm test && cd ..",
    "test:server": "mocha --exit",
    "build:client": "cd client && npm run build",
    "eslint": "./client/node_modules/eslint/bin/eslint.js ./*.js ./db/*.js ./db/*/*.js ./db/*/*/*.js ./test/*.js ./client/src/*.js ./client/src/*/*.js ./client/src/*/*/*.js"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.0",
    "express": "^4.16.3",
    "express-cors": "0.0.3",
    "jsonwebtoken": "^8.1.1",
    "jsrsasign": "^8.0.5",
    "knex": "^0.14.6",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "git+https://github.com/chaijs/chai-http.git#3ea4524",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0"
  },
